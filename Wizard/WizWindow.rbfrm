		  Timer1.Mode = Timer.ModeSingle
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Function SavePic(p As Picture) As FolderItem
		  Dim d As New Date
		  Dim ext As String
		  Select Case SaveType
		  Case Picture.SaveAsJPEG
		    ext = ".jpg"
		  Case Picture.SaveAsPNG
		    ext = ".png"
		  Case Picture.SaveAsWindowsBMP
		    ext = ".bmp"
		  End Select
		  Dim filename As String = Win32.CurrentUser + "_" + Format(d.TotalSeconds, "##############################") + ext
		  Dim f As FolderItem = SpecialFolder.Temporary.Child(filename)
		  p.Save(f, SaveType, Picture.QualityHigh)
		  App.DoEvents(500)
		  Return f
		  
		End Function
	#tag EndMethod


